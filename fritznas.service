<!DOCTYPE service-group SYSTEM "avahi-service.dtd">
<service-group>
<name replace-wildcards="yes">Fritz NAS</name>
<service>
	<type>_smb._tcp</type>
	<host-name>fritz.nas</host-name>
	<port>445</port>
</service>
<service>
	<type>_device-info._tcp</type>
	<host-name>fritz.nas</host-name>
	<port>0</port>
	<txt-record>model=Xserve</txt-record>
</service>
</service-group>
